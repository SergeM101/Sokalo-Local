<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.sokalo.controller.SystemLogController">
   <children>
      <VBox alignment="CENTER" layoutX="37.0" layoutY="18.0" prefHeight="363.0" prefWidth="525.0">
         <children>
            <Label text="Full System Log">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <TableView fx:id="logTableView" prefHeight="200.0" prefWidth="200.0">
               <columns>
                  <TableColumn fx:id="actionColumn" prefWidth="92.0" text="Action" />
                  <TableColumn fx:id="detailsColumn" prefWidth="171.33334350585938" text="Details" />
                  <TableColumn fx:id="staffColumn" prefWidth="135.33331298828125" text="Staff Member" />
                  <TableColumn fx:id="timestampColumn" prefWidth="122.66668701171875" text="Timestamp" />
               </columns>
            </TableView>
            <Button fx:id="printReportButton" mnemonicParsing="false" onAction="#handlePrintReportClick" text="Print Report" />
         </children>
      </VBox>
   </children>
</AnchorPane>
